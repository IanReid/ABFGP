#include "alignprots.h" 

std::string conservative_subs = "AS-DE-DN-EK-EQ-FW-FY-HN-HY-IL-IM-IV-KQ-KR-LM-LV-MV-NS-QR-ST-WY";

int alignproteins::score( int i, int j )
{
	int C1 = Sequence1.iSequence[ i - 1 ] - 65;
	int C2 = Sequence2.iSequence[ j - 1 ] - 65;
	 
	if ( C1 < 0 || C2 < 0 || C1 > 25 || C2 > 25 )
		return 0; // sorry but don't know
	
	if (C1 > C2 )
		return pMatrix[C2][C1];
	else
		return pMatrix[C1][C2];
}

char alignproteins::mType( int i, int j )
{
	char rl;
	char* ch = new char[3]; 
	//cout << "(" << i << "\t" << j << endl;
	if ( Sequence1.iSequence[ i - 1] > Sequence2.iSequence[ j - 1] )
	{
		ch[0] = Sequence2.iSequence[j - 1];
		ch[1] = Sequence1.iSequence[i - 1];
	}
	else
	{
		ch[0] = Sequence1.iSequence[i-1];
		ch[1] = Sequence2.iSequence[j-1];
	}
	
	ch[2] = 0;
	
	if ( conservative_subs.find( ch ) != std::string::npos )
	{
		rl = '+';
	}
	else
		if ( ch[0] == ch[1] )
			rl = ch[0];
		else rl = ' ';
	 
	delete[] ch;
	return rl;
}

void alignproteins::loadMatrix( proteinMatrix pM )
{
	for ( int x = 0; x != 26; x++ )
	{
		for ( int y = 0; y != 26; y++ )
		{
			pMatrix[x][y] = pM[x][y];
		}
	}
}


void alignproteins::loadInHouseMatrix( std::string mName )
{
	if ( mName == "blosum100" )
		loadMatrix(blosum100);
	if ( mName == "blosum30" )
		loadMatrix(blosum30);
	if ( mName == "blosum35" )
		loadMatrix(blosum35);
	if ( mName == "blosum40" )
		loadMatrix(blosum40);
	if ( mName == "blosum45" )
		loadMatrix(blosum45);
	if ( mName == "blosum50" )
		loadMatrix(blosum50);
	if ( mName == "blosum55" )
		loadMatrix(blosum55);
	if ( mName == "blosum60" )
		loadMatrix(blosum60);
	if ( mName == "blosum62" )
		loadMatrix(blosum62);
	if ( mName == "blosum65" )
		loadMatrix(blosum65);
	if ( mName == "blosum70" )
		loadMatrix(blosum70);
	if ( mName == "blosum75" )
		loadMatrix(blosum75);
	if ( mName == "blosum80" )
		loadMatrix(blosum80);
	if ( mName == "blosum85" )
		loadMatrix(blosum85);
	if ( mName == "blosum90" )
		loadMatrix(blosum90);
	if ( mName == "blosum95" )
		loadMatrix(blosum95);
	
	if ( mName == "pam120" )
		loadMatrix(pam120);
	if ( mName == "pam180" )
		loadMatrix(pam180);
	if ( mName == "pam250" )
		loadMatrix(pam250);
	if ( mName == "pam30" )
		loadMatrix(pam30);
	if ( mName == "pam300" )
		loadMatrix(pam300);
	if ( mName == "pam60" )
		loadMatrix(pam60); 
	if ( mName == "pam90" )
		loadMatrix(pam90); 
		
	MatrixName = mName;
}

void alignproteins::calcScores()
{
	alignment.gaps = cCount( alignment.mL,' ' );
	alignment.positives = cCount_NNot( alignment.mL, ' ' );
	alignment.identities = alignment.positives - cCount( alignment.mL,'+' );
}

void alignproteins::display_specific()
{
	cout << "matrix: " << MatrixName << endl;
}

